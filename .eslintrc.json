{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
        ],
        "linebreak-style": 0,
        "no-tabs": 0,
        "no-console": 0,
        "import/prefer-default-export": 0
    }
}
